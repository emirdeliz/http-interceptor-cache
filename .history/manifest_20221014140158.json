{
	"manifest_version": 3,
	"name": "Intercept HTTP and return from cache",
	"version": "1.1",
	"description": "Chrome extension to intercept HTTP requests and return from cache",
	"icons": {
		"16": "images/icon-16.png",
		"32": "images/icon-32.png",
		"48": "images/icon-48.png",
		"128": "images/icon-128.png"
	},
	"action": {},
	"content_scripts": [
		{
			"js": [
				"src/js/index.js"
			],
			"matches": [
				"<all_urls>"
			],
			"run_at": "document_start"
		}
	],
	"permissions": [
		"storage"
	],
	"background": {
		"service_worker": "src/js/http-interceptor-cache-service-worker.js",
		"type": "module"	
	},
	"key": "gbnjnikpmojonmejodjfemmbcmkcjkmi"
}